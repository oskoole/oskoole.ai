const OpenAI = require('openai');
const fs = require('fs');
const https = require('https');

const openai = new OpenAI({
  apiKey: 'YOUR_API_KEY_HERE'  // Replace with your actual API key
});

async function generateTutorialImage(prompt) {
  console.log('🎨 Generating image with DALL-E...');
  console.log('Prompt:', prompt);
  
  try {
    const response = await openai.images.generate({
      model: "dall-e-3",
      prompt: prompt,
      n: 1,
      size: "1024x1024",
      quality: "standard"  // or "hd" for better quality (costs 
more)
    });
    
    const imageUrl = response.data[0].url;
    console.log('✅ Image generated!');
    console.log('URL:', imageUrl);
    
    // Download the image
    const filename = 'dalle-tutorial-step.png';
    await downloadImage(imageUrl, filename);
    console.log(`✅ Image saved as ${filename}`);
    
    return imageUrl;
    
  } catch (error) {
    console.error('❌ Error:', error.message);
  }
}

// Helper function to download image
function downloadImage(url, filename) {
  return new Promise((resolve, reject) => {
    https.get(url, (response) => {
      const fileStream = fs.createWriteStream(filename);
      response.pipe(fileStream);
      fileStream.on('finish', () => {
        fileStream.close();
        resolve();
      });
    }).on('error', reject);
  });
}

// Test it with a Gmail tutorial prompt
generateTutorialImage(
  "A clean, professional screenshot of Gmail signup page showing 
input fields for first name and last name, with a red arrow 
pointing to the first name field, modern web interface design, 
high quality UI"
);
